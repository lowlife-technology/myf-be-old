generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Identity {
  id       String @id @default(uuid())
  email    String @unique
  fullName String
  password String @unique
}

model NewIncome {
  id        String   @id @default(uuid())
  Category  String
  CreatedAt DateTime @default(now())
  Amount    String

  @@map("incomes")
}
